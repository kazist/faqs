{# empty Twig template #}
{% import "form.macro.twig" as forms %}


<h1 class="component-header">{{ item.title }}</h1>

<div class="business-posted-date">
    <i class="fa fa-clock-o"></i> Posted:  {{ item.date_created|date('d M Y H:i') }} &nbsp;&nbsp;
</div>

<div>

    <div class="form-group">
        <label>Description</label><br>
        {{ item.description }}
    </div>

</div>

<div> 
    <div class="clearfix"></div>
    <div class="category-faqs-listing">
        {% include "faq.list.twig" ignore missing with {'category_id': item.id, 'action': 'featured'}  %}
    </div>
    <div class="clearfix"></div>
    <br>
</div>
